{
  "name": "openfront-discord-bot",
  "version": "1.4.1",
  "description": "OpenFront Discord bot",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "migrate": "wrangler d1 migrations apply DB --remote",
    "deploy": "wrangler deploy",
    "deploy-commands": "tsx src/scripts/deploy_commands.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260117.0",
    "@eslint/compat": "^1.2.3",
    "@eslint/js": "^9.11.0",
    "@swc/jest": "^0.2.29",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "eslint": "^9.11.0",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-sh": "^0.14.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0",
    "wrangler": "^4.62.0"
  },
  "dependencies": {
    "dedent": "^1.7.0",
    "discord-api-types": "^0.38.37",
    "discord-interactions": "^4.4.0",
    "discord.js": "^14.25.0",
    "workers-og": "^0.0.27"
  }
}
